<div class="main-container ace-save-state" id="main-container">
  <script type="text/javascript">
    try{ace.settings.loadState('main-container')}catch(e){}
  </script>

  <%- partial('./partials/sideBar.ejs') %>


  <div class="main-content">
    <div class="main-content-inner">
      <div class="breadcrumbs ace-save-state" id="breadcrumbs">


        <div class="nav-search" id="nav-search">
          <form class="form-search">
								<span class="input-icon">
									<input type="text" placeholder="Search ..." class="nav-search-input" id="nav-search-input" autocomplete="off" />
									<i class="ace-icon fa fa-search nav-search-icon"></i>
								</span>
          </form>
        </div><!-- /.nav-search -->
      </div>

      <div class="page-content">

        <div class="row">
          <div class="col-xs-12">
            <!-- PAGE CONTENT BEGINS -->
            <div class="row">
              <div class="col-xs-12">
                <div class="clearfix">
                  <div class="pull-right tableTools-container"></div>
                </div>
                <div class="table-header">
                 Manual Subscribe
                </div>

                <!-- div.table-responsive -->

                <!-- div.dataTables_borderWrap -->
                <div>
                  <table id="dynamic-table" class="table table-striped table-bordered table-hover">
                    <thead>
                    <tr>

                      <th>Subscribe Name</th>
                      <th>Status</th>
                      <th>Login Client</th>

                    </tr>
                    </thead>

                    <tbody>

                    <%
                    subscribe.forEach(function (current,index) {
                    %>

                    <tr>

                      <td><%=current.Feedsymbool%></td>
                      <td>
                        <span class="label label-sm label-success">Online</span>
                      </td>

                      <td>
                        <a class="green editClients" id="<%=current.id%>"  href="#">
                          <i class="ace-icon fa fa-plus bigger-130"> </i>
                        </a>
                      </td>

                    </tr>

                    <%  });
                    %>

                    </tbody>
                  </table>
                </div>
              </div>
            </div>

            <!-- PAGE CONTENT ENDS -->
          </div><!-- /.col -->
        </div><!-- /.row -->
      </div><!-- /.page-content -->
    </div>
  </div><!-- /.main-content -->
</div><!-- /.main-container -->
<script src="/js/jquery-1.11.3.min.js"></script>

<script>
    $(".editClients").click(function(){
        var id = $(this).attr("id");
        $.ajax({
            url: '/manulsubscribe',
            type: 'POST',
            data: {subscribe: id},
            success: function(response){
                swal("Subscribe Activate Successfully!");
            }
        });
    });

</script>